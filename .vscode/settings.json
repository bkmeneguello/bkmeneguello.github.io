{
    "frontMatter.content.autoUpdateDate": true,
    "frontMatter.framework.id": "other",
    "frontMatter.content.publicFolder": "",
    "[markdown]": {
        "editor.fontSize": 14,
        "editor.lineHeight": 26,
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 64,
        "editor.lineNumbers": "off",
        "editor.quickSuggestions": false,
        "editor.minimap.enabled": false
    },
    "frontMatter.preview.host": "http://localhost:4000/",
    "frontMatter.framework.startCommand": "docker run --rm -it -v \"$(pwd):/srv/jekyll\" -v \"jekyll_cache:/usr/local/bundle\" -p 4000:4000 -p 35729:35729 jekyll/jekyll jekyll serve --force_polling --livereload"
}